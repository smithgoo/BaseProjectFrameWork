# Uncomment the next line to define a global platform for your project
# platform :ios, '9.0'

target 'MainProject' do
    
    pod 'ViewUtils'
    #类似网易新闻的切换
    pod 'HMSegmentedControl'
    #加载框
    pod 'SVProgressHUD'
    #键盘处理
    pod 'IQKeyboardManager'
    #获取设备的唯一标识
    pod 'SecureUDID'
    #刷新
    pod 'MJRefresh'
    #约束
    pod 'Masonry'
    #模型解析扩展
    pod 'MJExtension'
    #相片读取
    pod 'FSMediaPicker'
    
    pod 'LinqToObjectiveC'
    pod 'DZNEmptyDataSet'
    
    #图片浏览器
    pod 'MWPhotoBrowser'
    pod 'VGParallaxHeader'
    #网络请求
    pod 'AFNetworking', '~> 3.1.0'
    #图片加载
    pod 'SDWebImage', '~> 3.7.3'
    #加载自定义弹框列表
    pod 'ActionSheetPicker-3.0', '~> 2.0.3'
    #tableviwe 算高度
    pod 'UITableView+FDTemplateLayoutCell', '~> 1.4.beta'
    #抽屉效果
    pod 'MMPopupView', '1.3'
    
    pod 'NYXImagesKit', '2.3'
    #RAC
    pod 'ReactiveCocoa', ‘~> 2.5’
    #scoket 心跳包 长连接
    pod 'RHSocketKit', '~> 2.1.0'
    #banner 无限轮播
    pod 'SDCycleScrollView', '~> 1.65'
    #阿里云OSS图片上传
    pod 'AliyunOSSiOS', '~> 2.6.0'
    #推送集成
    pod 'JPush', '~> 3.0.2'
    #倒计时按钮
    pod 'JKCountDownButton', '~> 1.0.0' #倒计时
    
    # 集成 分享请看这个 http://wiki.mob.com/cocoapods%E9%9B%86%E6%88%90sharesdk%E6%A0%87%E5%87%86%E7%89%88/
    # Pods for MainProject
    #加载gif图片
    pod 'YLGIFImage', '~> 0.11'
    #Tangram是一套简单可依赖的Javascript库
    pod 'Tangram'
    
    #统跳协议的方法
    pod 'JYRouter'
  
    
    #bug 腾讯异常追踪
    pod 'Bugly'
    
    
    #分享集成
    # 主模块(必须)
    pod 'ShareSDK3'
    # Mob 公共库(必须) 如果同时集成SMSSDK iOS2.0:可看此注意事项：http://bbs.mob.com/thread-20051-1-1.html
    pod 'MOBFoundation'
    
    # UI模块(非必须，需要用到ShareSDK提供的分享菜单栏和分享编辑页面需要以下1行)
    pod 'ShareSDK3/ShareSDKUI'
    
    # 平台SDK模块(对照一下平台，需要的加上。如果只需要QQ、微信、新浪微博，只需要以下3行)
    pod 'ShareSDK3/ShareSDKPlatforms/QQ'
    pod 'ShareSDK3/ShareSDKPlatforms/SinaWeibo'
    pod 'ShareSDK3/ShareSDKPlatforms/WeChat'



    
    
    # 扫描银行卡
    pod 'CardIO'
    
    #显示 尺寸和大小
    pod 'MMPlaceHolder'
    
    # YYkit
    
#    pod 'YYKit'

#    pod 'DDOpenCVOcrMaster'

    
    
end
